#include<iostream>
using namespace std;

struct stu
{
    int roll;
    float marks;
    char name[20];
};
int main()
{
    struct stu s;
     cout<<"Enter student roll no.:";
     cin>>s.roll;
     cout<<"enter marks of student:";
     cin>>s.marks;
     cout<<"enter name of student:";
     cin>>s.name;
     cout<<endl;
     cout<<s.roll<<endl;
     cout<<s.marks<<endl;
     cout<<s.name<<endl;
return 0;
}

